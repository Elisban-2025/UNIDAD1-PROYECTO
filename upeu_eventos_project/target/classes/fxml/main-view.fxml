<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/17"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="upeu.edu.pe.control.EventoController"
      spacing="10" alignment="CENTER" prefWidth="800" prefHeight="600">

    <TableView fx:id="tableEventos" prefHeight="500" prefWidth="700">
        <columns>
            <TableColumn fx:id="idCol" text="ID" prefWidth="100"/>
            <TableColumn fx:id="nombreCol" text="Nombre" prefWidth="200"/>
            <TableColumn fx:id="fechaCol" text="Fecha" prefWidth="150"/>
            <TableColumn fx:id="lugarCol" text="Lugar" prefWidth="200"/>
        </columns>
    </TableView>

</VBox>
